{
  "name": "storybook-expo",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:devclient": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "update-stories": "sb-rn-get-stories --config-path .ondevice",
    "prestart": "yarn update-stories",
    "storybook-watcher": "sb-rn-watcher --config-path .ondevice",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install",
    "commit": "git-cz",
    "format": "prettier --write ."
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "resolutions": {
    "@types/react": "17.0.39"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-community/datetimepicker": "6.1.2",
    "@react-native-community/slider": "4.2.1",
    "@storybook/addon-actions": "~6.5",
    "@storybook/addon-controls": "~6.5",
    "@storybook/addon-ondevice-actions": "next",
    "@storybook/addon-ondevice-backgrounds": "next",
    "@storybook/addon-ondevice-controls": "next",
    "@storybook/addon-ondevice-notes": "next",
    "@storybook/react-native": "next",
    "expo": "^45.0.0",
    "expo-splash-screen": "~0.15.1",
    "expo-status-bar": "~1.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-web": "^0.18.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@storybook/addon-essentials": "~6.5",
    "@storybook/addon-links": "~6.5",
    "@storybook/addon-react-native-web": "^0.0.18",
    "@storybook/react": "~6.5",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.67.6",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "^5.23.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-react-native-web": "^0.17.5",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "latest",
    "eslint-plugin-storybook": "^0.5.13",
    "git-cz": "^4.9.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "~4.3.5"
  },
  "private": true
}
